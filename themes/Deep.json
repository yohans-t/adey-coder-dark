  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#293D29"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "meta.definition.type.body.swift",
        "meta.block variable.other",
        "meta.item-access.python",
        "source.kotlin variable.parameter"
      ],
      "settings": {
        "foreground": "#9BFF9B"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color",
        "meta.function-call.js",
        "meta.method.declaration.js"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF83a0"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": [
        "storage.modifier",
        "storage.modifier.java",
        "storage.type.js",
        "keyword.control",
        "meta.use.php keyword.other",
        "keyword.other.directive.using.cs",
        "keyword.type.go",
        "keyword.other.rust",
        "keyword.other.new",
        "keyword.operator.new",
        "keyword.other.kotlin",
        "storage.type.function.python",
        "keyword.other.declaration-specifier.swift",
        "storage.control.clojure",
        "source.sml storage.type",
        "storage.type.kotlin"
      ],
      "settings": {
        "foreground": "#FFE496"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "meta.class.identifier.java",
        "meta.class.java"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.operator.class.php",
        "storage.type.function.arrow.js"
      ],
      "settings": {
        "foreground": "#DEFFDE"
      }
    },

    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#FFFFFF99"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#9BFF9B"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": ["meta.function-call", "keyword.other.special-method"],
      "settings": {
        "foreground": "#F0FF77"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function",
        "meta.function-call",
        "entity.name.function.js",
        "entity.global.clojure"
      ],
      "settings": {
        "foreground": "#9BFF9B"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "variable.other.property.js",
        "meta.object-literal.key.js",
        "variable.other.member.c",
        "variable.other.property.ts",
        "variable.other.object.property.js",
        "entity.name.variable.parameter.php",
